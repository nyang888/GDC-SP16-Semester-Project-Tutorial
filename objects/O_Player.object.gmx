        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// CODE RUN AT CREATION

image_speed=.25;
xSpeed=0;
ySpeed=0;
grounded=false;
ms=3;
jump_max=5;
jump_timer=jump_max;
grav=.5;
//contolling shot speed
fire_delay=0;
fire_delay_max=4;
//gives the bullet this speed
shot_speed=16;

//Variable for overheating
overheat = 1;
overheat_max = 10;
overheat_increment = .5;
overheat_decrement = .15;

//keeping track of direction
